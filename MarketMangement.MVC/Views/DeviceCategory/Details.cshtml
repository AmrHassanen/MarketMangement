@model DeviceCategory

@{
    ViewData["Title"] = "Device Category Details";
}

<h1>Device Category Details</h1>

<div>
    <h4>Device Category</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">ID</dt>
        <dd class="col-sm-10">@Model.ID</dd>

        <dt class="col-sm-2">Name</dt>
        <dd class="col-sm-10">@Model.CategoryName</dd>


    </dl>
</div>

<p>
    <a href="@Url.Action("Edit", "DeviceCategory", new { id = Model.ID })" class="btn btn-warning">Edit</a>
    <a href="@Url.Action("Index", "DeviceCategory")" class="btn btn-secondary">Back to List</a>
</p>
